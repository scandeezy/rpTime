<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Landing</servlet-name>
		<servlet-class>com.roosterpark.rptime.LandingServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>User</servlet-name>
		<servlet-class>com.roosterpark.rptime.UserServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Sheet</servlet-name>
		<servlet-class>com.roosterpark.rptime.SheetServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>History</servlet-name>
		<servlet-class>com.roosterpark.rptime.HistoryServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Landing</servlet-name>
		<url-pattern>/rptime</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>User</servlet-name>
		<url-pattern>/rptime/user</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Sheet</servlet-name>
		<url-pattern>/rptime/sheet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>History</servlet-name>
		<url-pattern>/rptime/history</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
